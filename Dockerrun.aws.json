{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "postgres_db"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "asengame"
                }
            ],
            "essential": true,
            "image": "postgres:12.7-alpine",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/psql/data",
                    "sourceVolume": "VarLibPsqlData"
                }
            ],
            "name": "db",
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "REDIS_HOST",
                    "value": "redis-server"
                },
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                }
            ],
            "essential": true,
            "image": "redis:6.2-alpine",
            "name": "redis-server",
            "memory": 120
        },
        {
            "command": [
                "pytest",
                "./puppycompanyblog/test.py"
            ],
            "essential": true,
            "mountPoints": [
                {
                    "containerPath": "/app/main.py",
                    "sourceVolume": "_Main_Py"
                },
                {
                    "containerPath": "/app/puppycompanyblog",
                    "sourceVolume": "_Puppycompanyblog"
                }
            ],
            "name": "tests"
        },
        {
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "asengame"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "db"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "5432"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "postgres_db"
                }
            ],
            "essential": true,
            "mountPoints": [
                {
                    "containerPath": "/app/main.py",
                    "sourceVolume": "_Main_Py"
                },
                {
                    "containerPath": "/app/puppycompanyblog",
                    "sourceVolume": "_Puppycompanyblog"
                }
            ],
            "name": "web",
            "image": "asengame/flask-docker:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./main.py"
            },
            "name": "_Main_Py"
        },
        {
            "host": {
                "sourcePath": "./puppycompanyblog/"
            },
            "name": "_Puppycompanyblog"
        },
        {
            "host": {
                "sourcePath": "/var/lib/psql/data"
            },
            "name": "VarLibPsqlData"
        }
    ]
}


