{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [{
			"environment": [{
					"name": "POSTGRES_USER",
					"value": $POSTGRES_USER
				},
				{
					"name": "POSTGRES_PASSWORD",
					"value": $POSTGRES_PASSWORD
				},
				{
					"name": "POSTGRES_DB",
					"value": $POSTGRES_DB
				}
			],
			"essential": true,
			"image": "postgres:12-alpine",
			"memory": 100,
			"mountPoints": [{
				"containerPath": "/var/lib/postgresql/data/"
			}],
			"name": "db",
			"portMappings": [{
				"containerPort": 5432,
				"hostPort": 5432
			}]
		},
		{
			"essential": true,
			"image": "redis:6.0.8",
			"memory": 100,
			"name": "redis",
			"portMappings": [{
				"containerPort": 6379,
				"hostPort": 6379
			}]
		},
		{
			"essential": true,
			"links": [
				"redis",
				"db"
			],
			"name": "web",
			"image": "asengame/flask-docker",
			"memory": 100,
			"portMappings": [{
				"containerPort": 80,
				"hostPort": 80
			}]
		}
	]
}

