{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "postgres_db"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "postgres"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "asengame"
                }
            ],
            "essential": true,
            "image": "postgres:12.7-alpine",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/psql/data",
                    "sourceVolume": "VarLibPsqlData"
                }
            ],
            "name": "db",
	    "memory": 100,
            "portMappings": [
                {
                    "containerPort": 5432,
                    "hostPort": 5432
                }
            ]
        },
        {
            "environment": [
                {
                    "name": "REDIS_HOST",
                    "value": "redis-server"
                },
                {
                    "name": "REDIS_PORT",
                    "value": "6379"
                }
            ],
            "essential": true,
            "image": "redis:6.2-alpine",
            "name": "redis-server",
            "memory": 100
        },
        {
            "essential": true,
	    "links": [
		"redis-server",
		"db"
            ],
            "image": "asengame/flask-docker",
            "name": "web",
	    "memory": 100,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/lib/psql/data"
            },
            "name": "VarLibPsqlData"
        }
    ]
}

